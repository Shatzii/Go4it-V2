  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3000",
    "dev:replit": "next dev -H 0.0.0.0 -p 5000",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:production": "cross-env NODE_ENV=production SKIP_TYPE_CHECK=true NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "node server.js",
    "start:production": "NODE_ENV=production PORT=5000 HOSTNAME=0.0.0.0 node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:production": "tsc --project tsconfig.production.json --noEmit",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
